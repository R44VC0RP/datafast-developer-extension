{
  "manifest_version": 3,
  "name": "Datafast Developer Disabler",
  "version": "1.0",
  "description": "Toggle Datafast analytics tracking for development purposes",
  "permissions": [
    "storage",
    "activeTab",
    "scripting"
  ],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "128": "icons/icon.png"
    }
  },
  "background": {
    "service_worker": "background.js"
  },
  "host_permissions": [
    "<all_urls>"
  ]
} 